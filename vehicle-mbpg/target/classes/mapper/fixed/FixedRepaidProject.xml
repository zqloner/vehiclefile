<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mgl.dao.fixed.FixedRepaidProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mgl.bean.fixed.FixedRepaidProject">
        <id column="id" property="id" />
        <result column="project_name" property="projectName" />
        <result column="cost_of_price" property="costOfPrice" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_name, cost_of_price, del_flag
    </sql>

</mapper>
